<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" coreApp="true" package="com.car.cloud" platformBuildVersionCode="22" platformBuildVersionName="5.1-1568030917">
    <uses-permission android:name="android.permission.DEVICE_POWER"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_MULTICAST_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.INTERACT_ACROSS_USERS_FULL"/>
    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:name=".CloudApplication" android:theme="@style/AppTheme">
        <receiver android:name="com.car.cloud.SystemEventReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </receiver>
        <activity android:label="@string/app_name" android:name="com.car.cloud.CarCloudActivity" android:windowSoftInputMode="stateAlwaysHidden"/>
        <activity android:excludeFromRecents="true" android:name=".AdActivity" android:theme="@style/ad_activity">
            <intent-filter>
                <action android:name="com.car.ad.show"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.car.cloud.QRActivity"/>
        <provider android:authorities="com.car.cloud.cloudmsgprovider" android:exported="true" android:name="com.car.cloud.CloudMsgProvider"/>
        <service android:name="com.car.cloud.CarCloudService">
            <intent-filter>
                <action android:name="com.car.cloud.CarCloudService"/>
                <action android:name="com.car.cloud.CheckVersion"/>
            </intent-filter>
            <intent-filter>
                <action android:name="com.car.cloud.RemindVersion"/>
            </intent-filter>
        </service>
    </application>
</manifest>